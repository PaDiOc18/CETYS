<UserControl x:Class="Stacks.Nodin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Stacks"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="50">
    <Grid>
        <Ellipse x:Name="circulo" Fill="#FF080808" Stroke="Black" Height="50" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="50"/>
        <Label x:Name="Numerito" Margin="0,0,9.6,14.6" Foreground="White" FontWeight="Bold" FontSize="10" Background="{x:Null}" Content=":0" Height="22" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="21"/>

    </Grid>
</UserControl>
